{
  "manifest_version": 3,
  "name": "Roblox Community Rich Player Finder",
  "version": "1.0",
  "description": "Find the richest players in Roblox communities based on their limiteds worth over 10,000 Robux",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://groups.roblox.com/*",
    "https://users.roblox.com/*",
    "https://inventory.roblox.com/*",
    "https://api.roblox.com/*",
    "https://economy.roblox.com/*",
    "https://catalog.roblox.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Roblox Rich Player Finder"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}