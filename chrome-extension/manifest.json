{
  "manifest_version": 3,
  "name": "Roblox Richest Players Finder",
  "version": "1.0",
  "description": "Finds richest players in a Roblox group based on limiteds valued over 10,000 Robux.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Roblox Rich List"
  },
  "permissions": ["storage"],
  "host_permissions": [
    "https://groups.roblox.com/*",
    "https://inventory.roblox.com/*",
    "https://economy.roblox.com/*"
  ]
}